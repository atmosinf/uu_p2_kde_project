@prefix ex: <http://example.org/movie/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# Ontology header
ex:MovieRecOntology a owl:Ontology ;
  rdfs:label "Movie Recommendation Ontology" .

# Classes
ex:Movie    a owl:Class .
ex:Person   a owl:Class .
ex:Actor    a owl:Class ; rdfs:subClassOf ex:Person .
ex:Director a owl:Class ; rdfs:subClassOf ex:Person .
ex:Genre    a owl:Class .

# Object
ex:actor a owl:ObjectProperty ;
  rdfs:domain ex:Movie ;
  rdfs:range  ex:Actor .

ex:director a owl:ObjectProperty ;
  rdfs:domain ex:Movie ;
  rdfs:range  ex:Director .

ex:genre a owl:ObjectProperty ;
  rdfs:domain ex:Movie ;
  rdfs:range  ex:Genre .

# Datatype
ex:title a owl:DatatypeProperty ;
  rdfs:domain ex:Movie ;
  rdfs:range xsd:string .

ex:year a owl:DatatypeProperty ;
  rdfs:domain ex:Movie ;
  rdfs:range xsd:gYear .

ex:runtime a owl:DatatypeProperty ;
  rdfs:domain ex:Movie ;
  rdfs:range xsd:integer .

#Restrictions
ex:Movie rdfs:subClassOf [
  a owl:Restriction ;
  owl:onProperty ex:title ;
  owl:someValuesFrom rdfs:Literal
] .

ex:Movie rdfs:subClassOf [
  a owl:Restriction ;
  owl:onProperty ex:year ;
  owl:someValuesFrom rdfs:Literal
] .